{
    "manifestVersion": 1,
    "id": "adoinstallmodule",
    "name": "ADO Install Module",
    "version": "1.0.0",
    "publisher": "pattontech",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "An Azure DevOps PowerShell Task to install a module",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "adoinstallmodule/icon.png"
    },
    "files": [
        {
            "path": "adoinstallmodule"
        }
    ],
    "contributions": [
        {
            "id": "adoinstallmodule",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "adoinstallmodule"
            }
        }
    ]
}